int border = 20;

void setup() {
  size (700,700);
  background (255);
  stroke (0);
  strokeWeight (1.2);
  
  // x coordinate 
  for (int i=0; i<25; i++) { //start,stop,step
    int x = (i*34);
    
      //y coordinate
      for (int j=0; j<25; j++) {
        int y = (j*34);
      
        //line (50, 100, 10, 10); //line(x1, y1, x2, y2) //realized this would not work
        line(x+10+border, y+border, x+10+border, y+20+border); //realized border had to be used, 
        //and something had to be added to x & y
        line(x+border, y+10+border, x+20+border, y+10+border);
      
     
    }
  }
}
  
  
