int hi = 13;
int border = 50;


void setup() {
  size (600,600);
  background(255);
  stroke(-1);
  
  rectMode(CENTER);

      //change the x coordinate
  for(int i=0; i<25; i++) { //start,stop, step
    // change the y coordniate
    for(int j=0;j<25;j++) {
         fill(random(255),random(255),random(255));

        rect(border+(i*20),border+(j*20),hi,hi); // x coord, y coord, width, height
  
      }
    
  }
 
}
